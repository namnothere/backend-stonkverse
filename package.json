{
  "name": "stonkverse-server",
  "version": "1.0.0",
  "description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"200\" alt=\"Nest Logo\" /></a> </p>",
  "main": ".eslintrc.js",
  "bin": {
    "stonkverse-server": "bin/NamingStrategy.js"
  },
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "typeorm": "typeorm-ts-node-commonjs",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migrate:generate": "typeorm-ts-node-commonjs migration:generate -d bin/ormconfig.ts src/migration/migration",
    "migrate:create": "cd ./src/migration && typeorm-ts-node-commonjs migration:create",
    "migrate:up": "typeorm-ts-node-commonjs migration:run -d bin/ormconfig.ts",
    "migrate:down": "typeorm-ts-node-commonjs migration:revert -d bin/ormconfig.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/core": "^0.1.33",
    "@langchain/openai": "^0.0.14",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.11",
    "eslint-config-prettier": "^9.1.0",
    "langchain": "^0.1.21",
    "nanoid": "^3.3.4",
    "nestjs-pino": "^4.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pino-pretty": "^10.3.1",
    "prompts": "^2.4.2",
    "shortid": "^2.2.16",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^10.3.3",
    "@types/node": "^20.11.20",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20"
  }
}
